<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" xmlns="http://agavi.org/agavi/config/parts/routing/1.0">
	<ae:configuration context="web">
		<routes>
		
			<!-- inGraph specific routes (include on the fly) -->
			<route name=".ingraph" pattern="^/ingraph" stop="false" module="InGraph">
				<route name=".provider" pattern="^/provider" stop="false" output_type="json">
					<route name=".test" pattern="^/test" action="Provider.Test" />
					<route name=".hosts" pattern="^/hosts" action="Provider.Hosts" />
					<route name=".services" pattern="^/services" action="Provider.Services" />
				</route>
			</route>
		
		</routes>
	</ae:configuration>
</ae:configurations>