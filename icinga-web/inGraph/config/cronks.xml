<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0" xmlns="http://icinga.org/cronks/config/parts/cronks/1.0">
    <ae:configuration>
        
        <categories>
        
            <category name="inGraph">
            
                <ae:parameter name="title">inGraph</ae:parameter>
                <ae:parameter name="visible">true</ae:parameter>
                <ae:parameter name="position">30</ae:parameter>
            </category>
        </categories>
        
        <cronks>

            <cronk name="inGraph">
            
                <ae:parameter name="module">inGraph</ae:parameter>
                <ae:parameter name="action">Cronk.ClassicView</ae:parameter>
                <ae:parameter name="hide">false</ae:parameter>
                <ae:parameter name="description">Seamless inGraph integration</ae:parameter>
                <ae:parameter name="name">inGraph</ae:parameter>
                <ae:parameter name="categories">ingraph</ae:parameter>
                <ae:parameter name="image">cronks.Stats2</ae:parameter>
            </cronk>
            
            <cronk name="inGraph single">
            
                <ae:parameter name="module">inGraph</ae:parameter>
                <ae:parameter name="action">Cronk.SingleView</ae:parameter>
                <ae:parameter name="hide">false</ae:parameter>
                <ae:parameter name="description">Seamless inGraph integration</ae:parameter>
                <ae:parameter name="name">inGraph single</ae:parameter>
                <ae:parameter name="categories">ingraph</ae:parameter>
                <ae:parameter name="image">cronks.Stats2</ae:parameter>
            </cronk>
        </cronks>
    </ae:configuration>
</ae:configurations>
