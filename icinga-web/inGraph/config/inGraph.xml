<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://agavi.org/agavi/config/parts/module/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0">
    <setting name="xmlrpc">
        <ae:parameter name="host">127.0.0.1</ae:parameter>
        <ae:parameter name="port">5000</ae:parameter>
        <ae:parameter name="user">ingraph</ae:parameter>
        <ae:parameter name="pass">ingraph</ae:parameter>
        <ae:parameter name="timeout">10</ae:parameter>
    </setting>
    <setting name="daemon">
        <ae:parameter name="nullTolerance">0</ae:parameter>
    </setting>
    <setting name="templates">
        <ae:parameter name="dir">
            %core.module_dir%/inGraph/config/templates</ae:parameter>
        <ae:parameter name="default">default.json</ae:parameter>
    </setting>
    <setting name="views">
        <ae:parameter name="dir">
            %core.module_dir%/inGraph/config/views</ae:parameter>
    </setting>
</settings>