{
    "re": "//",

    "flot": {
        "series": {
            "lines": {
                "show": true,
                "steps": true,
                "lineWidth": 2
            }
        },
        "legend": {
            "position": "nw"
        }
    },

    "series": [
        {
            "re": "//",
            "type": "avg",
            "lines": {
                "fill": true,
                "lineWidth": 2,
                "fillColor": {
                    "colors": [
                        {
                            "opacity": 0.7
                        },
                        {
                            "opacity": 0.9,
                            "brightness": 0.9
                        }
                    ]
                }
            }
        }
    ],

    "panels": [
        {
            "title": "Four Hours (With Quickview) Graph For {host}<tpl if=\"service\"> - {service}</tpl>",
            "overviewConfig": {
                "enable": true
            },
            "start": "-4 hours"
        },
        {
            "title": "One Day Graph For {host}<tpl if=\"service\"> - {service}</tpl>",
            "start": "-1 day"
        },
        {
            "title": "One Week Graph For {host}<tpl if=\"service\"> - {service}</tpl>",
            "start": "-1 week"
        },
        {
            "title": "One Month Graph For {host}<tpl if=\"service\"> - {service}</tpl>",
            "start": "-1 month"
        },
        {
            "title": "One Year Graph For {host}<tpl if=\"service\"> - {service}</tpl>",
            "start": "-1 year"
        }
    ]
}
