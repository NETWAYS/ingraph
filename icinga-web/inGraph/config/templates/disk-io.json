{
    "re": "/^disk-io/i",

    "flot": {
        "series": {
            "lines": {
                "show": true,
                "fill": true,
                "lineWidth": 0,
                "fillColor": {
                    "colors": [
                        {
                            "opacity": 0.4
                        },
                        {
                            "opacity": 0.9,
                            "brightness": 0.8
                        }
                    ]
                }
            }
        },

        "yaxes": [
            {
                "label": "I/O",
                "unit": "byte"
            }
        ]
    },

    "series": [
        {
            "color": "#2D7DB3",
            "re": "/read/i",
            "type": "avg",
            "label": "Read",
            "stack": "io",
            "yaxis": 1,
            "lines": {
                "fillColor": {
                    "colors": [
                        {
                            "opacity": 0.7
                        },
                        {
                            "opacity": 1,
                            "brightness": 0.9
                        }
                    ]
                }
            }
        },
        {
            "color": "#844798",
            "re": "/write/i",
            "type": "avg",
            "label": "Write",
            "stack": "io",
            "yaxis": 1
        },
        {
            "color": "#AB1852",
            "re": "/total/i",
            "type": "avg",
            "label": "Total",
            "lines": {
                "fill": null,
                "lineWidth": 2
            },
            "yaxis": 1
        }
    ]
}