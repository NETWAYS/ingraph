{
    "re": "/^http/i",

    "flot": {
        "yaxes": [
            {
                "position": "left",
                "label": "Time",
                "unit": "time"
            },
            {
                "position": "right",
                "tickColor": "#AB1852",
                "label": "Size",
                "unit": "byte"
            }
        ]
    },

    "series": [
        {
            "color": "#FF6E00",
            "label": "Size",
            "yaxis": 2,
            "type": "avg",
            "re": "/size/i",
            "lines": {
                "fill": null
            }
        },
        {
            "color": "#FFAA00",
            "label": "Time",
            "yaxis": 1,
            "type": "avg",
            "re": "/time/i",
            "lines": {
                "lineWidth": 0
            }
        }
    ]
}
