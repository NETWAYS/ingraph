{
    "re": "/bufferpool-hitrate/i",

    "flot": {
        "series": {
            "lines": {
                "show": true,
                "fill": true,
                "fillColor": {
                    "colors": [
                        {
                            "opacity": 0.4
                        },
                        {
                            "opacity": 0.9,
                            "brightness": 0.8
                        }
                    ]
                }
            }
        },

        "yaxes": [
            {
                "label": "Bufferpool"
            }
        ]
    },

    "series": [
        {
            "color": "#49A835",
            "re": "/bufferpool_hitrate$/i",
            "type": "avg",
            "id": "avg",
            "yaxis": 1,
            "label": "Hitrate",
            "fillBetween": "min",
            "lines": {
                "fillColor": {
                    "colors": [
                        "rgba(189, 189, 189, 0.4)",
                        "rgba(189, 189, 189, 0.9)"
                    ]
                }
            }
        },
        {
            "color": "#BDBDBD",
            "re": "/bufferpool_hitrate$/i", 
            "type": "min",
            "id": "min",
            "yaxis": 1,
            "label": "Min",
            "lines": {
                "lineWidth": 0,
                "fill": null
            }
        },
        {
            "color": "#BDBDBD",
            "re": "/bufferpool_hitrate$/i",
            "type": "max",
            "id": "max",
            "yaxis": 1,
            "label": "Max",
            "fillBetween": "avg",
            "lines": {
                "lineWidth": 0,
                "fillColor": {
                    "colors": [
                        "rgba(189, 189, 189, 0.4)",
                        "rgba(189, 189, 189, 0.9)"
                    ]
                }
            }
        },
        {
            "color": "#FF6E00",
            "re": "/bufferpool_hitrate$/i", 
            "type": "warn_upper",
            "id": "warn",
            "fillBetween": "max",
            "label": "Warning",
            "yaxis": 1
        }, 
        {
            "color": "#CB0C29",
            "re": "/bufferpool_hitrate$/i", 
            "type": "crit_upper",
            "fillBetween": "warn",
            "label": "Critical",
            "yaxis": 1
        }
    ]
}
