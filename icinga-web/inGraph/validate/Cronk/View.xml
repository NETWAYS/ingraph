<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
parent="%core.module_dir%/Reporting/config/validators.xml">
    <ae:configuration>
        <validators>
            <validator class="string" name="cmpid" required="false">
                <argument>cmpid</argument>
            </validator>
            <validator class="string" name="parentid" required="false">
                <argument>parentid</argument>
            </validator>
            <validator class="string" name="stateuid" required="false">
                <argument>stateuid</argument>
            </validator>
            <validator class="string" name="state" required="false">
                <argument>state</argument>
            </validator>
            <validator class="string" name="host" required="false">
                <argument>host</argument>
            </validator>
            <validator class="string" name="service" required="false">
                <argument>service</argument>
            </validator>
            <validator class="string" name="view" required="false">
                <argument>view</argument>
            </validator>
            <validator class="boolean" name="hideMenu" required="false">
                <argument>hideMenu</argument>
            </validator>
        </validators>
    </ae:configuration>
</ae:configurations>