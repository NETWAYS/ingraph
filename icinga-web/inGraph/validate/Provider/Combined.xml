<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
    parent="%core.module_dir%/inGraph/config/validators.xml"
>
    
    <ae:configuration>
        
        <validators>
        
            <validator name="config" class="string" required="true">
                <argument>config</argument>
            </validator>
            
            <validator name="start" class="number" required="false">
                <argument>start</argument>
            </validator>
            
            <validator name="end" class="number" required="false">
                <argument>end</argument>
            </validator>
            
            <validator name="interval" class="number" required="false">
                <argument>interval</argument>
            </validator>
        
            <validator name="nullTolerance" class="number" required="false">
                <argument>nullTolerance</argument>
            </validator>
        </validators>
        
    </ae:configuration>
    
</ae:configurations>