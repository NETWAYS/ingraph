<?xml version="1.0" encoding="UTF-8"?>
<ae:configurations
    xmlns="http://agavi.org/agavi/config/parts/validators/1.0"
    xmlns:ae="http://agavi.org/agavi/config/global/envelope/1.0"
    parent="%core.module_dir%/inGraph/config/validators.xml"
>
    <ae:configuration>
        <validators>
            <validator name="name" class="string" required="true">
                <argument>name</argument>
                <error>Parameter name is mandatory.</error>
            </validator>
            <validator name="content" class="string" required="true">
                <argument>content</argument>
                <error>Parameter content is mandatory.</error>
            </validator>
        </validators>
    </ae:configuration>
</ae:configurations>
