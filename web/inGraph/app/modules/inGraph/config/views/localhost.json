{
"title" : "Localhost View",
"panels": [{
    "title" : "PING / HTTP -1 Year",
    "start": "-1 year",
    "series": [{
        "host": "localhost",
        "service": "PING",
        "re": "/^pl$/",
        "type": "avg",
        "label": "Packet Loss",
        "color": "#FF0000"
    }, {
        "host": "localhost",
        "service": "PING",
        "re": "/^rta$/",
        "type": "avg",
        "label": "Round Trip Time",
        "color": "#00FF00"
    }, {
        "host": "localhost",
        "service": "HTTP",
        "re": "//",
        "type": "avg"
    }]
}, {
    "title" : "Load / Procs -24 Hours",
    "start": "-24 hours",
    "series": [{
        "host": "localhost",
        "service": "Current Load",
        "re": "//",
        "type": "avg"
    }, {
        "host": "localhost",
        "service": "Total Processes",
        "re": "//",
        "type": "avg"
    }]
}]
}